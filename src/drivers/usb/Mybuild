package embox.driver.usb

module core {
	option number log_level = 1

	source "usb_core.c",
		"usb_queue.c",
		"usb_config.c"

	source "usb_driver.c"
	depends embox.driver.char_dev
}

module hub {
	option number log_level = 1

	source "usb_hub.c"

	depends core
}

module driver_example {
	source "usb_driver_example.c"

	depends core
}
